<?xml version="1.0" encoding="UTF-8"?>
<widget id="widget.mobileopenlaszlo.org/opera/laszloweather" version="1.0 beta"
  dockable="@dockable@" floatable="@floatable@" network="public"> 
  <widgetname>@widgetname@</widgetname> 
  <description>@description@</description> 
  <icon>@icon@</icon>
  <width>@width@</width> 
  <height>@height@</height> 
  <author>
    <name>@author.name@</name> 
    <organization>@author.organization@</organization> 
    <link>@author.link@</link> 
    <email>@author.email@</email>
  </author>
  <security>
	<access>
	  <protocol>http</protocol>
	  <host>weather.yahooapis.com</host>
	  <host>l.yimg.com</host>
	  <host>weather.yahoo.com</host>
	</access>
  </security>
  <!-- Disallow browser plugins -->
  <content plugins="no" />
  <!--update url="@update._url@"></update-->
  <license>@license@</license>
</widget>
